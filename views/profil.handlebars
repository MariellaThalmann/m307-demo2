<!--My Profil Anzeige-->
<h2>My Profil</h2>
{{#users}}
  <div class="user">
    <h4>{{username}}</h4>
    <h4>{{prename}}</h4>
    <h4>{{name}}</h4>
    <h4>{{age}}</h4>
    <h4>{{phonenumber}}</h4>
    <h4>{{email}}</h4>
  </div>
{{/users}}

<!-- 26.11.2024 Posts Anzeige -->
{{#posts}}
  <div class="post">
    <img src="/uploads/{{image}}" alt="" />

    <div class="innercontainer">
      <small>{{date}}</small>
      <h1>{{username}}</h1>
      <h2>{{title}}</h2>
      <p>{{description}}</p>
      <a href="/post/{{id}}">Details</a>
      <!--Likes_neu-->
      <form action="/like/{{id}}" enctype="multipart/form-data" method="POST">
        <input class="heart" type="image" src="/heart.png" />
      </form>
    </div>
  </div>
{{/posts}}