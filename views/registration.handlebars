<!--Registrations Formular-->
<h2>Registrieren</h2>
<form action="/registration" method="POST">
  <input name="username" type="text" placeholder="username" /><br />
  <input name="prename" type="text" placeholder="prename" /><br />
  <input name="name" type="text" placeholder="name" /><br />
  <input name="age" type="int" placeholder="age" /><br />
  <input name="phonenumber" type="text" placeholder="phonenumber" /><br />
  <input name="email" type="text" placeholder="email" /><br />
  <input name="passwort" type="password" placeholder="password" /><br />
  <input
    class="Button-Form"
    class="registration"
    type="submit"
    value="Registrieren"
  />
</form>